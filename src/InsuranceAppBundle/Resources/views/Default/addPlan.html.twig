{% extends 'InsuranceAppBundle:Default:layout.html.twig' %}
{% block stylesheets %}{% endblock %}

{% block body %}

<div class="container contcustomhead headchat">
    <div class="row">
        <div id="hide">
            <a href="javascript:history.go(-1)">
                <div class="col-xs-1 ch">
                    <span class="fa fa-chevron-left fa-2x"></span>
                </div>
                <div class="col-xs-2 ba">
                    <p>Back</p>
                </div>
            </a>
        </div>
        <div class="col-xs-6 tiaddplan">
            <h2 id="tiindexh2">Add Plan</h2>
        </div>
        <div class="col-xs-3 logcorner">
            <img id="log" src="{{ asset('bundles/insuranceapp/img/Wandersafe logo_icon-01.png') }}"/>
        </div>
    </div>
</div>

<div class="container contaddcustom">
    <div class="row">

        <div class="col-xs-12 btnsearch">
            <button type="button" class="btn btn-default">
                <a href="{{ path('insurance_app_search') }}" class="hover"><span class="fa fa-search fa-2x span"></span>Search</a>
            </button>
        </div>

        <div class="col-xs-12 btnsearch">
            <button type="button" class="btn btn-default">
                <a href="{{ path('insurance_app_qrcode') }}" class="hover"><span class="fa fa-qrcode fa-2x span"></span>QR code</a>
            </button>
        </div>

        <div class="col-xs-12 btnsearch">
            <button type="button" class="btn btn-default">
                <a href="{{ path('insurance_app_nearme') }}" class="hover"><span class="fa fa-map-marker fa-2x span"></span>Near me</a>
            </button>
        </div>

    </div>
</div>

{% endblock %}